    "eslint-plugin-promise": "^4.3.1",
    "style-loader": "^3.1.0",
    "terser-webpack-plugin": "^5.1.4",
    "ts-loader": "^9.2.3",
    "vue": "^3.0.5",
    "vue-loader": "^16.3.1",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.32.0",
